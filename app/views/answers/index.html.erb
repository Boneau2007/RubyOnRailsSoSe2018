<p id="notice"><%= notice %></p>

<h1>Answers</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th>Positiv rating</th>
      <th>Negative rating</th>
      <th>Staranswer</th>
      <th>Question</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%= answer.text %></td>
        <td><%= answer.positiv_rating %></td>
        <td><%= answer.negative_rating %></td>
        <td><%= answer.staranswer %></td>
        <td><%= answer.question %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
